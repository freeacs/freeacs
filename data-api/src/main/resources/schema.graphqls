type Unit {
    id: String!
    profile: Profile!
    unittype: UnitType!
}

type Profile {
    id: Int!
    name: String!
}

type UnitType {
    id: Int!
    name: String!
    vendor: String!
    description: String
    protocol: String!
}

type Query {
    getUnits(search: String!, unittypeName: String, profileName: String, limit: Int!): [Unit]
}

schema {
    query: Query
}
