<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.github.freeacs</groupId>
    <artifactId>parent</artifactId>
    <version>2.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>FreeACS Parent</name>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <commons.httpclient.version>3.1</commons.httpclient.version>
        <apache.httpclient.version>4.5.5</apache.httpclient.version>
        <freemarker.version>2.3.14</freemarker.version>
        <javax.mail.version>1.4.1</javax.mail.version>
        <javax.activation.version>1.1</javax.activation.version>
        <jaxrpc.version>1.1</jaxrpc.version>
        <axis.version>1.4</axis.version>
        <servlet.version>2.5</servlet.version>
        <spring.version>3.1.4.RELEASE</spring.version>
        <spring.webflow.version>2.2.1.RELEASE</spring.webflow.version>
        <mysql.connector.version>8.0.11</mysql.connector.version>
        <jfreechart.version>1.0.17</jfreechart.version>
        <docker.image.prefix>freeacs</docker.image.prefix>
    </properties>

    <scm>
        <connection>scm:git:git://github.com/freeacs/freeacs.git</connection>
        <developerConnection>scm:git:git@github.com:freeacs/freeacs.git</developerConnection>
        <tag>HEAD</tag>
        <url>https://github.com/freeacs/freeacs/</url>
    </scm>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.5.3</version>
            </plugin>
        </plugins>
    </build>

    <modules>
        <module>common</module>
        <module>dbi</module>
        <module>tr069</module>
        <module>syslog</module>
        <module>stun</module>
        <module>shell</module>
        <module>core</module>
        <module>web</module>
        <module>wsc</module>
        <module>ws</module>
    </modules>

</project>
